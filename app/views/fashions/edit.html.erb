<%= form_for @fashion do |f| %>
    <%= f.label :name %>
    <%= f.label :address %>
    <%= f.label :about %>
    <%= f.label :image %>
    <%= f.text_field :name, :size => 140 %>
    <%= f.text_field :address, :size => 140 %> 
    <%= f.text_field :about, :size => 140 %> 
    <%= f.text_field :image, :size => 140 %> 
    <%= f.submit "編集する" %>
  <% end %>